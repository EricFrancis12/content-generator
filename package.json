{
  "name": "docker-content-generator",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "lint": "eslint . --ext .ts --ext .tsx",
    "test": "jest",
    "preinstall": "node services/node-app/scripts/_prestart.js",
    "postinstall": "npm run install:_shared && npm run install:apply-filters-engine && npm run install:campaign-engine && npm run install:cleanup-engine && npm run install:download-engine && npm run install:node-app && npm run install:publish-engine && npm run install:ui",
    "install:_shared": "cd services/_shared && npm install",
    "install:apply-filters-engine": "cd services/apply-filters-engine && npm install",
    "install:campaign-engine": "cd services/campaign-engine && npm install",
    "install:cleanup-engine": "cd services/cleanup-engine && npm install",
    "install:download-engine": "cd services/download-engine && npm install",
    "install:node-app": "cd services/node-app && npm install",
    "install:publish-engine": "cd services/publish-engine && npm install",
    "install:ui": "cd services/ui && npm install",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "ts-jest": "^29.1.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,html,css}": "eslint"
  }
}