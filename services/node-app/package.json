{
    "name": "node-app",
    "version": "1.0.0",
    "main": "build/index.js",
    "scripts": {
        "start": "node scripts/_prestart.js && node build/src/index.js",
        "build": "npx tsc",
        "dev": "node scripts/_prestart.js && nodemon -L -e ts --exec \"tsc -p ./tsconfig.json && node ./build/src/index.js\""
    },
    "dependencies": {
        "@tsconfig/node18": "^18.2.2",
        "@types/amqplib": "^0.10.5",
        "@types/express": "^4.17.21",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.11.30",
        "amqplib": "^0.10.3",
        "express": "^4.18.2",
        "mongoose": "^8.2.2",
        "typescript": "^5.4.3"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.1.0"
    }
}